{
  "apiVersion": "tanka.dev/v1alpha1",
  "kind": "Environment",
  "metadata": {
    "name": "local-dev"
  },
  "spec": {
    "apiServer": "https://127.0.0.1:6443",
    "namespace": "spezistudyplatform",
    "resourceDefaults": {
      "labels": {
        "environment": "local-dev"
      }
    },
    "expectVersions": {
      "kubernetes": "1.29"
    },
    "injectLabels": true,
    "diffStrategy": "server-side",
    "applyStrategy": "server-side",
    "garbageCollection": true
  }
}