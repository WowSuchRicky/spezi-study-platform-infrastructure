apiVersion: v1
kind: Secret
metadata:
  name: oauth2-proxy-secret
  namespace: spezistudyplatform
type: Opaque
stringData:
  client-id: oauth2-proxy
  # retrieved this from keycloak, can be automated eventually too but it's just a one time thing for now. 
  # https://study.muci.sh/auth/admin/master/console/#/spezistudyplatform/clients/<autogenerated-id>/credentials
  client-secret: retrieve_from_kc
  # Generate cookie-secret with `dd if=/dev/urandom bs=32 count=1 2>/dev/null | base64 | tr -d -- '\n' | tr -- '+/' '-_' ; echo`
  cookie-secret: generate_with_above_command